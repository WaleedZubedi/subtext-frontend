{
  "expo": {
    "name": "SubText",
    "slug": "subtext-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "subtext",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "primaryColor": "#000000",
    "backgroundColor": "#000000",
    "description": "AI-powered conversation analysis. Decode hidden intentions and manipulation tactics in text messages.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.subtext.ai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.subtext.ai"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "SubText - AI Conversation Analysis",
      "shortName": "SubText",
      "description": "Decode hidden intentions in conversations with AI",
      "backgroundColor": "#000000",
      "themeColor": "#6200ea",
      "lang": "en",
      "scope": "/",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
